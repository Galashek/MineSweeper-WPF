<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net5.0-windows</TargetFramework>
    <UseWPF>true</UseWPF>
    <ApplicationIcon>Images\mine.ico</ApplicationIcon>
    <Win32Resource />
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Images\clock.png" />
    <None Remove="Images\flag.png" />
    <None Remove="Images\mine.png" />
    <None Remove="Images\reset.png" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\.editorconfig" Link=".editorconfig" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\MineSweeper\MineSweeper.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Images\clock.png" />
    <Resource Include="Images\flag.png" />
    <Resource Include="Images\mine.png" />
    <Resource Include="Images\reset.png" />
  </ItemGroup>

</Project>
